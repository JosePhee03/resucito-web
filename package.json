{
  "name": "resucito-web",
  "version": "1.0.0",
  "description": "Proyecto web que busca llevar el libro de cantos RESUCITO (en espa√±ol) al mundo digital a traves de la web",
  "main": "index.js",
  "workspaces": [
    "backend",
    "frondend"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/JosePhee03/resucito-web"
  },
  "scripts": {
    "start:backend": "pnpm run --filter backend start",
    "dev:frontend": "pnpm run --filter frontend dev",
    "dev": "pnpm run dev:backend && dev:frontend",
